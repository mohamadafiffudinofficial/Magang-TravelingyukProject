<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.code.optimasiproject">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.OptimasiProject">
        <activity
            android:name="com.code.optimasiproject.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.code.optimasiproject.AddProjectActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.code.optimasiproject.UpdateProjectActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.code.optimasiproject.DetailProjectActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.code.optimasiproject.AddKeywordActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.code.optimasiproject.UpdateKeywordActivity"
            android:screenOrientation="portrait"/>
    </application>

</manifest>